t = int(input())
for test_case in range(1, t + 1):
    string = input()
    string = " " + string
    string = list(string)
    n = int(input())
    array = list(map(int, input().split()))
    for i in array:
        string[i] = string[i] + "-"
    result = ("".join(string)).strip()

    print(f"#{test_case}", result)
